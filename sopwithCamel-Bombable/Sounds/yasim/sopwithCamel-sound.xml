<?xml version="1.0"?>

<PropertyList>

  <fx>

    <crank>
      <name>crank</name>
      <path>Sounds/coughing.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine/cranking</property>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume>
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
    </crank>

    <engine>
      <name>engine-blip</name>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/leclerget-blip.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/controls/engines/engine/blip_switch</property>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <!--<orientation>
        <x>-0.75</x>
        <y>0.0</y>
        <z>-0.17</z>
        <inner-angle>30.0</inner-angle>
        <outer-angle>100.0</outer-angle>
        <outer-gain>0.0</outer-gain>
      </orientation>-->
      <volume>
        <property>/engines/engine/mp-osi</property>
        <factor>0.035</factor>
        <min>0.15</min>
        <max>1.0</max>
        <offset>0.15</offset>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.0008</factor>
        <min>.3</min>
        <max>1</max>
        <offset>0.3</offset>
      </pitch>
    </engine>

    <engine>
      <name>engine</name>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/leclerget.wav</path>
      <mode>looped</mode>
      <property>/engines/engine/running</property>
      <condition>
        <not>
          <property>/controls/engines/engine/blip_switch</property>
        </not>
        <property>/engines/engine/running</property>
      </condition>
      <!--<orientation>
        <x>-0.75</x>
        <y>0.0</y>
        <z>-0.17</z>
        <inner-angle>30.0</inner-angle>
        <outer-angle>100.0</outer-angle>
        <outer-gain>0.0</outer-gain>
      </orientation>-->
      <volume>
        <property>/engines/engine/mp-osi</property>
        <factor>0.035</factor>
        <min>0.15</min>
        <max>1.0</max>
        <offset>0.05</offset>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.008</factor>
        <min>.03</min>
        <max>1</max>
        <offset>0.3</offset>
      </pitch>
    </engine>



    <birds>
      <name>birdsong</name>
      <mode>looped</mode>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/birdsong.wav</path>
      <!--<path>Aircraft/MPCarrier/Sounds/anchorsAweigh.wav</path>-->
      <!--<property>/velocities/airspeed-kt</property>-->
      <condition>
        <equals>
          <property>/sim/startup/season</property>
          <value>summer</value>
        </equals>
      </condition>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.0010</factor>
        <offset>0.1</offset>
        <min>0.01</min>
        <max>0.1</max>
      </volume>
    </birds>

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.005</factor>
        <min>0.001</min>
        <max>1.5</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>2.25</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>

    <overspeed>
      <name>overspeed</name>
      <mode>looped</mode>
      <path>Sounds/wind1.wav</path>
      <condition>

                 <greater-than>
                               <property>/velocities/airspeed-kt</property>
                               <value>140</value>
                 </greater-than>

      </condition>      
      
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <offset>-0.21</offset>
        <min>0.01</min>
        <max>2.0</max>
      </volume>

      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.025</factor>        
      </pitch>
    </overspeed>    

    <overspeed2>
      <name>overspeed2</name>
      <mode>looped</mode>
      <path>Sounds/wind-8khz.wav</path>
      <condition>

                 <greater-than>
                               <property>/velocities/airspeed-kt</property>
                               <value>160</value>
                 </greater-than>

      </condition>      
      
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <offset>-0.24</offset>
        <min>0.01</min>
        <max>2.0</max>
      </volume>
      </overspeed2>
      

  
      <overspeed3>
      <name>overspeed3</name>
      <mode>looped</mode>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/176235__soundslikewillem__wind-turbine.wav</path>
      <condition>

                 <greater-than>
                               <property>/velocities/airspeed-kt</property>
                               <value>140</value>
                 </greater-than>

      </condition>      
      
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.035</factor>
        <offset>-3.25</offset>
        <min>0.01</min>
        <max>5.0</max>
      </volume>
      

      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.007</factor>        
      </pitch>
      
    </overspeed3>    
    
    <highg>
      <name>highg</name>
      <mode>once</mode>
      <path>Sounds/crash.wav</path>
      <condition>
       <or>
                 <greater-than>
                               <property>/fdm/jsbsim/accelerations/Nz</property>
                               <value>4</value>
                 </greater-than>                                            
                 <lesser-than>
                               <property>/fdm/jsbsim/accelerations/Nz</property>
                               <value>-3</value>
                 </lesser-than>
       </or>
      </condition>
      <volume>
        <property>/fdm/jsbsim/accelerations/Nz</property>
        <factor>0.05</factor>
        <offset>0.1</offset>
        <min>0.1</min>
        <max>0.4</max>
      </volume>

      <pitch>
        <property>/fdm/jsbsim/accelerations/Nz</property>
        <factor>0.05</factor>
        <offset>0</offset>
      </pitch>
    </highg>
    

    <guns>
      <name>gunfire</name>
      <mode>looped</mode>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/vickers.wav</path>
      <condition>
        <property>controls/armament/trigger</property>
        <property>ai/submodels/submodel/count</property>
      </condition>
      <volume>
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
    </guns> 
    
    <crash1>
      <name>crashed1</name>
      <mode>once</mode>
      <path>Sounds/crash.wav</path>
      
      <condition>                                                     
                 <property>/sim/crashed</property>                     
      </condition>      
      <volume>
        <min>1</min>
        <max>1</max>
      </volume>
    </crash1>  
    <crash1b>
      <name>crashed1b</name>
      <mode>once</mode>
      <path>Sounds/thunder3.wav</path>
      
      <condition>                                                     
                 <property>/sim/crashed</property>                     
      </condition>      
      <volume>
        <min>1</min>
        <max>1</max>
      </volume>
    </crash1b>        
    
    <crash1c>
      <name>crashed1c</name>
      <mode>once</mode>
      <path>Sounds/wave.wav</path>      
      <condition>                                                     
                 <property>/fdm/jsbsim/systems/crash-detect/impact-water</property>                     
      </condition>      
      <volume>
        <min>1</min>
        <max>1</max>
      </volume>
    </crash1c>      
  </fx>

</PropertyList>
