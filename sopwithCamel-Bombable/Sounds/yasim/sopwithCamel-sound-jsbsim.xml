<PropertyList>

  <fx>

    <crank>
      <name>crank</name>
      <path>Sounds/coughing.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine/cranking</property>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume>
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
    </crank>

    <engineblip>
      <name>engine-blip</name>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/leclerget-blip.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/controls/engines/engine/blip_switch</property>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <volume>
        <property>/engines/engine/mp-osi</property>
        <factor>0.035</factor>
        <min>0.15</min>
        <max>1.0</max>
        <offset>0.15</offset>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.0008</factor>
        <min>.3</min>
        <max>1</max>
        <offset>0.3</offset>
      </pitch>
    </engineblip>

   
    <engine>
      <name>engine</name>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/leclerget.wav</path>
      <mode>looped</mode>
      <property>/engines/engine/running</property>
      <condition>
        <not>
          <property>/controls/engines/engine/blip_switch</property>
        </not>
        <property>/engines/engine/running</property>
      </condition>
      <volume>
        <property>/engines/engine/mp-osi</property>
        <factor>0.035</factor>
        <min>0.15</min>
        <max>1.0</max>
        <offset>0.05</offset>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.008</factor>
        <min>.03</min>
        <max>1</max>
        <offset>0.3</offset>
      </pitch>
    </engine>
  
    <!-- x front, z up, y left in YASim & Y right in JSBsim, not sure which it is here -->
    
    <lwheelroll>
      <name>leftwheelroll</name>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/220841__robertwulfman__low-rumble.wav</path>
      <mode>looped</mode>

      <condition>
        <and>
          <greater-than> 
                         <property>/gear/gear/compression-ft</property>
                         <value>0.05</value>
          </greater-than>
          <greater-than> 
                         <property>/gear/gear/rollspeed-ms</property>
                         <value>0.1</value>
          </greater-than>
        </and>
      </condition>
         <position> 
              <x>0.5</x>
              <y>1.0</y>
              <z>-1.5</z>
      </position>
      
      <volume>
        <property>/gear/gear/rollspeed-ms</property> 
        <factor>0.1</factor>
        <min>0</min>              
        <max>1.0</max>
        <offset>0</offset>
      </volume>
      
      <pitch>
        <property>/gear/gear/rollspeed-ms</property>
        <factor>0.2</factor>
        <min>.0001</min>
        <max>2</max>
        <offset>0.1</offset>
      </pitch>
      
    </lwheelroll>    
    <rwheelroll>
      <name>rightwheelroll</name>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/220841__robertwulfman__low-rumble.wav</path>
      <mode>looped</mode>

      <condition>
        <and>
          <greater-than> 
                         <property>/gear/gear[1]/compression-ft</property>
                         <value>0.05</value>
          </greater-than>
          <greater-than> 
                         <property>/gear/gear[1]/rollspeed-ms</property>
                         <value>0.1</value>
          </greater-than>
        </and>
      </condition>
         <position> 
              <x>0.5</x>
              <y>-1.0</y>
              <z>-1.5</z>
      </position>
      
      <volume>
        <property>/gear/gear[1]/rollspeed-ms</property> 
        <factor>0.1</factor>
        <min>0</min>              
        <max>1.0</max>
        <offset>0</offset>
      </volume>
      
      <pitch>
        <property>/gear/gear[1]/rollspeed-ms</property>
        <factor>0.2</factor>
        <min>.0001</min>
        <max>2</max>
        <offset>0.1</offset>
      </pitch>
    </rwheelroll>    

    <!-- gear[3] will compress during water landing OR hard ground landing so this sound should be appropriate either way -->
    <waterlanding>
      <name>waterlanding</name>
      <path>Sounds/wind.wav</path>      
      <mode>looped</mode>

      <condition>
        <and>
          <greater-than> 
                         <property>/gear/gear[3]/compression-ft</property>
                         <value>0.15</value>
          </greater-than>
          <greater-than> 
                         <property>/gear/gear[3]/rollspeed-ms</property>
                         <value>0.1</value>
          </greater-than>
        </and>
      </condition>
      <position> 
              <x>0.5</x>
              <y>0.0001</y>
              <z>-1.5</z>
      </position>
      
      <volume>
        <property>/gear/gear[3]/rollspeed-ms</property> 
        <factor>0.1</factor>
        <min>0</min>              
        <max>1.0</max>
        <offset>0</offset>
      </volume>
      
      <pitch>
        <property>/gear/gear[3]/rollspeed-ms</property>
        <factor>0.2</factor>
        <min>.0001</min>
        <max>2</max>
        <offset>0.1</offset>
      </pitch>
      
    </waterlanding>    
    <waterlanding2>
      <name>waterlanding2</name>
      <path>Sounds/wave.wav</path>      
      <mode>looped</mode>

      <condition>
        <and>
          <greater-than> 
                         <property>/gear/gear[3]/compression-ft</property>
                         <value>0.03</value>
          </greater-than>
          <greater-than> 
                         <property>/gear/gear[3]/rollspeed-ms</property>
                         <value>0.05</value>
          </greater-than>
          <equals> 
           <property>/environment/terrain-info/terrain</property> <!-- 1 = land, 0 = water -->
           <value>0</value>
          </equals>
        </and>
      </condition>
         <position> 
              <x>0.5</x>
              <y>0.0001</y>
              <z>-1.5</z>
      </position>
      
      <volume>
        <property>/gear/gear[3]/rollspeed-ms</property> 
        <factor>0.05</factor>
        <min>0</min>              
        <max>1.0</max>
        <offset>0</offset>
      </volume>
      
      <pitch>
        <property>/gear/gear[3]/rollspeed-ms</property>
        <factor>0.07</factor>
        <min>.0001</min>
        <max>2</max>
        <offset>0.1</offset>
      </pitch>
      
    </waterlanding2>       
     
     
    <taildrag>
      <name>taildrag</name>
      <!-- <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/220841__robertwulfman__low-rumble.wav</path> -->
      <path>Sounds/wind-8khz.wav</path>
      <mode>looped</mode>

      <condition>
        <and>
          <greater-than> 
                         <property>/gear/gear[4]/compression-ft</property>
                         <value>0.06</value>
          </greater-than>
          <greater-than> 
                         <property>/gear/gear[4]/rollspeed-ms</property>
                         <value>0.1</value>
          </greater-than>           
          
          <!-- <property>/gear/gear[4]/wow</property> -->
          
        </and>
      </condition>
         <position> <!-- x front, z up, y left, as in YASim -->
              <x>-11.5</x>
              <y>0.01</y>
              <z>-1.5</z>
      </position>
      
      
      <volume>
        <property>/gear/gear[4]/rollspeed-ms</property> 
        <factor>0.1</factor>
        <min>0</min>              
        <max>1.0</max>
        <offset>0</offset>
      </volume>
      
      <pitch>
        <property>/gear/gear[4]/rollspeed-ms</property>
        <factor>0.25</factor>
        <min>.0001</min>
        <max>2</max>
        <offset>0.1</offset>
      </pitch>
      
    </taildrag>

    <taildrag2>
      <name>taildrag2</name>
      <path>Sounds/rumble-rough.wav</path>      
      <mode>looped</mode>

      <condition>
        <and>
          <greater-than> 
                         <property>/gear/gear[4]/compression-ft</property>
                         <value>0.06</value>
          </greater-than>
          <greater-than> 
                         <property>/gear/gear[4]/rollspeed-ms</property>
                         <value>0.1</value>
          </greater-than>
       </and>           
          
          <!-- <property>/gear/gear[4]/wow</property>        </and> -->
      </condition>
         <position> <!-- x front, z up, y left, as in YASim -->
              <x>-11.5</x>
              <y>.001</y>
              <z>-1.5</z>
      </position>
      
      
      <volume>
        <property>/gear/gear[4]/rollspeed-ms</property> 
        <factor>0.08</factor>
        <min>0</min>              
        <max>1.0</max>
        <offset>0</offset>
      </volume>
      
      <pitch>
        <property>/gear/gear[4]/rollspeed-ms</property>
        <factor>0.25</factor>
        <min>.0001</min>
        <max>2</max>
        <offset>0.1</offset>
      </pitch>
      
    </taildrag2>         
  
    <birds>
      <name>birdsong</name>
      <mode>looped</mode>
      <path>Aircraft/sopwithCamel-Bombable/Sounds/yasim/birdsong.wav</path>
      <!--<path>Aircraft/MPCarrier/Sounds/anchorsAweigh.wav</path>-->
      <!--<property>/velocities/airspeed-kt</property>-->
      <condition>
        <equals>
          <property>/sim/startup/season</property>
          <value>summer</value>
        </equals>
      </condition>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.0010</factor>
        <offset>0.1</offset>
        <min>0.01</min>
        <max>0.1</max>
      </volume>
    </birds>

  <groundscrape>
   <name>groundscrape</name>
   <mode>looped</mode>
   <path>Sounds/wind-8khz.wav</path>
   <condition>
    <and>
     <or>
       <!-- note that all these engage some distance out from the edge of the actual model , so wow will be true & this sound will play when these items are even CLOSE to the ground, not just on it. I guess that is OK for now. -->
       <property>gear/gear[5]/wow</property>
       <property>gear/gear[6]/wow</property>
       <property>gear/gear[7]/wow</property>
       <property>gear/gear[8]/wow</property>
       <property>gear/gear[9]/wow</property>
       <property>gear/gear[10]/wow</property>
       <property>gear/gear[11]/wow</property>
       <property>gear/gear[12]/wow</property>
       <property>gear/gear[13]/wow</property>
       <property>gear/gear[14]/wow</property>
       <property>gear/gear[15]/wow</property>
       <property>gear/gear[16]/wow</property>
       <property>gear/gear[17]/wow</property>
       <property>gear/gear[18]/wow</property>
       <property>gear/gear[19]/wow</property>
       <property>gear/gear[20]/wow</property>
       <property>gear/gear[21]/wow</property>
       <property>gear/gear[22]/wow</property>
       <property>gear/gear[23]/wow</property>     
     </or>
     <greater-than>
          <property>velocities/groundspeed-kt</property>
          <value>0.3</value>
     </greater-than>
    </and>
    <!-- <property>sim/current-view/internal</property> -->
   </condition>
   <position> <!-- x front, z up, y left in YASim & Y right in JSBsim, not sure which it is here -->
    <x>.5</x>
    <y>1.0</y>
    <z>-1.5</z>
   </position>
   <volume>
    <property>velocities/groundspeed-kt</property>
    <type>log</type>
    <factor>1</factor>
   </volume>
   <pitch>
    <property>velocities/groundspeed-kt</property>    
    <factor>0.1</factor>
    <max>1</max>
    <min>0.2</min>
    <offset>0</offset>
   </pitch>
  </groundscrape>

    
    <wind>
      <name>wind</name>
      <mode>looped</mode>      
      <path>Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.005</factor>
        <min>0.001</min>
        <max>1.5</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>2.25</factor>
        <offset>1.25</offset>
      </pitch>
    </wind>
    
     
  <buffet>
   <name>alpha-buffet</name>
   <mode>looped</mode>
       <name>wind</name>
   <condition>
    <and>
      <greater-than>
       <property>/fdm/jsbsim/aero/alpha-rad</property>
       <value>0.09</value>
      </greater-than>
      <greater-than>
        <property>/velocities/airspeed-kt</property>
        <value>25</value>
      </greater-than>
     </and> 
   </condition>
   <path>Sounds/wind.wav</path>
   <volume>
    <property>/fdm/jsbsim/aero/alpha-rad</property>
    <offset>-0.1</offset>
    <factor>1.0</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/fdm/jsbsim/aero/alpha-rad</property>
    <factor>0.1</factor>
   </pitch>
  </buffet>
  
  <buffetbeta>
   <name>beta-buffet</name>
   <mode>looped</mode>
   <name>wind</name>
   <condition>
    <and>
      <greater-than>
       <property>/fdm/jsbsim/aero/beta-rad</property>
       <value>0.09</value>
      </greater-than>
      <greater-than>
        <property>/velocities/airspeed-kt</property>
        <value>25</value>
      </greater-than>
     </and>
   </condition>
   <path>Sounds/wind.wav</path>
   <volume>
    <property>/fdm/jsbsim/aero/beta-rad</property>
    <offset>-0.1</offset>
    <factor>1.0</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/fdm/jsbsim/aero/beta-rad</property>
    <factor>0.1</factor>
   </pitch>
  </buffetbeta>
  
       
